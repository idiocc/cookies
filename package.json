{
  "name": "@goa/cookies",
  "version": "1.0.0",
  "description": "[fork] Signed And Unsigned Cookies Based On Keygrip Written In ES6 And Optimised With JavaScript Compiler.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "compile": "depack-dev src/depack -o compile/cookies.js -c -a -p",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md -n _goa",
    "d1": "typal types/index.js compile/index.js src/index.js -t types/index.xml,types/attributes.xml,types/keygrip.xml -c",
    "d": "yarn-s d1 externs",
    "externs": "typal types/externs/*",
    "build": "yarn-s d b doc",
    "e": "alanode"
  },
  "files": [
    "compile",
    "src",
    "types"
  ],
  "externs": [
    "types/externs/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/cookies.git"
  },
  "keywords": [
    "cookies",
    "goa",
    "http"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiocc/cookies/issues"
  },
  "homepage": "https://github.com/idiocc/cookies#readme",
  "devDependencies": {
    "@contexts/http": "^1.5.1",
    "alamode": "^2.3.4",
    "connect": "^3.7.0",
    "documentary": "^1.27.7",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  }
}
